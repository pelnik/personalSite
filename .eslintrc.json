{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["public/*"],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "react/jsx-filename-extension": "off",
        "import/no-cycle": "off",
        "react/no-array-index-key": "off",
        "import/extensions": "off",
        "no-unused-vars": "off",
        "no-console": "off",
    },
    "settings": {
        "import/resolver": {
          "node": {}
        }
      }
}
