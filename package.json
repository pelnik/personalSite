{
  "name": "personalsite",
  "version": "2.0.0",
  "description": "My personal site in React",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "start": "PORT=3001 react-scripts start",
    "start:dev": "NODE_ENV=development nodemon server.js",
    "start:express": "NODE_ENV=development nodemon server.js",
    "start:prod": "NODE_ENV=production sudo node server.js",
    "start:prod:full": "NODE_ENV=production git pull && npm install && react-scripts build && sudo node server.js",
    "seed:dev": "nodemon ./src/api/juicebox/db/seed.js",
    "seed": "node ./src/api/juicebox/db/seed.js",
    "test": "npm run start:dev & npm run test:fitness:all && npm run test:others",
    "test:fitness": "JEST_ENV=fitness jest --runInBand",
    "test:fitness:safe": "JEST_ENV=fitness jest --runInBand",
    "test:fitness:all": "JEST_ENV=fitness jest --runInBand fitness-tracker",
    "test:others": "JEST_ENV=others jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pelnik/personalSite.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Matt Pelnik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pelnik/personalSite/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/joy": "^5.0.0-alpha.72",
    "@mui/material": "^5.11.14",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-jest": "^29.5.0",
    "faker": "^5.5.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.22",
    "react-test-renderer": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
